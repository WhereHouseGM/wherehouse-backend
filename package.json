    "@types/bluebird": "^3.5.32",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "@types/supertest": "^2.0.10",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2"
  },
